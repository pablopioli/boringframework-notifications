    <RazorLangVersion>3.0</RazorLangVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/pablopioli/boringframework-notifications</PackageProjectUrl>
    <RepositoryUrl>https://github.com/pablopioli/boringframework-notifications</RepositoryUrl>
